
<% if in?  %>
    <% if current_guy.can_complete?(ch) %>

    <div class="complete_<%= ch.id %>">

      <%	url= "/complete/" +  ch.id.to_s	%>
      <%= form_for(ch, :url => url,
                   html: { method: :post }, class: 'complete_form',
                   remote: true) do |f| %>
          <% if current_guy.complete?(ch) then %>
          <%= f.submit "Undo", class: "button white nice small" %>
          <% else %>
              <%= f.submit "Complete", class: "button nice small" %>
          <% end %>
      <% end %>
    </div>

    <% end %>
<% end %>
